	Journal = {Educational Technology Research and Development},
	Keywords = {Educational Technology, Learning \& Instruction},
	Language = {en},
	Month = sep,
	Number = {3},
	Pages = {15--25},
	Title = {Audio-video correspondence and its role in attention and memory},
	Volume = {38},
	Year = {1990},
	Bdsk-Url-1 = {http://link.springer.com/article/10.1007/BF02298178},
