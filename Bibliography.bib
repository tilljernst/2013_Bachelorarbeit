	Journal = {Personality and Individual Differences},
	Number = {3},
	Pages = {213--217},
	Title = {{Attentional control moderates the relationship between activation of the cognitive attentional syndrome and symptoms of psychopathology}},
	Volume = {53},
	Year = {2012},
	Bdsk-Url-1 = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS&CSC=Y&NEWS=N&PAGE=fulltext&D=psyc7&AN=2012-15282-007},
