@article{Fergus2012,
	Abstract = {Wells's (2009) metacognitive theory suggests that inflexible and recurrent styles of thinking in response to negative thoughts, feelings, and beliefs exacerbate symptoms of psychopathology. Such styles of thinking underlie the cognitive attentional syndrome ({CAS).} Using a large nonclinical sample (N = 456), the present study examined whether attentional control moderates the relationship between activation of the {CAS} and symptoms of psychopathology (i.e., depression, anxiety, and stress symptoms). Consistent with predictions, relationships between activation of the {CAS} and assessed symptoms became increasingly stronger as attentional control decreased. Thus, for individuals who have a relative inability to disengage and shift attention from threat information (i.e., low attentional control), use of {CAS-relevant} coping strategies (e.g., rumination, worry) appears to be associated with especially deleterious psychological effects. Conceptual and therapeutic implications are discussed. ({PsycINFO} Database Record (c) 2012 {APA}, all rights reserved) (journal abstract).},
	Author = {Fergus, Thomas and Bardeen, Joseph and Orcutt, Holly},
	Date-Added = {2014-05-26 07:00:36 +0000},
